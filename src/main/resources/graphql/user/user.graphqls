type User {
    id: ID!
    name: String!
    email: String!
    createdAt: String!
    role: Role!
}

type Role {
    id: ID!
    name: String!
    description: String
}

input UserInput {
    name: String!
    email: String!
    createdAt: String!
    role: RoleInput!
}

input RoleInput{
    id: ID!
}