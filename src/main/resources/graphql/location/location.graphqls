type Location {
    id: ID!
    user: User!
    packag: Package!
    latitude: Float
    longitude: Float
    updatedAt: String!
    address: String!
}

input InputLocationAdd {
    user: UserInputAux!
    packag: PackageInput!
    latitude: Float
    longitude: Float
    address: String!
}

input InputLocationUpdate {
    id: ID!
    user: UserInputAux
    latitude: Float
    longitude: Float
    address: String
}