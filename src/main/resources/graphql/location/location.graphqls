type Location {
    id: ID!
    handlerUser: User!
    packageEntity: Package!
    latitude: Float
    longitude: Float
    updatedAt: String!
    address: String!
}

input InputLocationAdd {
    handlerUser: UserInputAux!
    packageEntity: PackageInput!
    latitude: Float
    longitude: Float
    address: String!
}

input InputLocationUpdate {
    id: ID!
    handlerUser: UserInputAux
    latitude: Float
    longitude: Float
    address: String
}